/* 轮播图 */
.index_seiper swiper{
  height: 340rpx;
  width: 750rpx;
}
.index_seiper image{
  width: 100%;
}
/* 导航 */
.index_cate{
display: flex;
}
.index_cate navigator{
 flex: 1;
 padding:20rpx;
}
.index_cate image{
  width: 100%;
}
/* 楼层 */
.floor_title{
  padding:10rpx 0;
}
.floor_title image{
  width: 100%;
}
.floor_list{
  /* 清除浮动 */
  overflow: hidden;
}
.floor_list navigator{
  float: left;
  width: 33.33%;
}
.floor_list navigator:nth-last-child(-n + 4){
  /* 第一张图 232*386  
  第一张图的高 height:33.33vw*386/232 */
  /* height: calc(33.33vw * 386 / 232 / 2); */
  height: calc(33.33vw * 386 / 232 / 2);
  border-left:10rpx solid #798a9b ;
}
.floor_list navigator:nth-child(2),
.floor_list navigator:nth-child(3)
{
 border-bottom: 10rpx solid #8e9fb1;
}
.floor_list image{
  width: 100%;
  height: 100%;
}