.order_item{
    padding: 20rpx;
    border-bottom: 1rpx solid #ccc;
    color:#666;
}
/* 订单编号 */
.order_no_row{
    display: flex;
    padding:10rpx 0;
    justify-content: space-between;
}
/* 订单价格 */
.order_price_row{
    display: flex;
    padding:10rpx 0;
    justify-content: space-between;
}
.order_price_value{
    color:var(--themeColor);
    font-size: 32rpx;
}
/* 订单日期 */
.order_time_row{
    display: flex;
    padding:10rpx 0;
    justify-content: space-between;

}